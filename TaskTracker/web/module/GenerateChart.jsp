                                    <tr>
                                        <td valign="top"><strong>Client Description</strong></td>
                                        <td><textarea id="taClientDescription" cols="23" rows="2" name="taClientDescription" readonly="readonly"></textarea></td>
                                    </tr>
                                </table>
                            </fieldset>
                        </td>
                    </tr>
                   
                </table>
            </td>
        </tr>
    </table>
